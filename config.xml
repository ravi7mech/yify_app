<?xml version='1.0' encoding='utf-8'?>
<widget id="com.project.yifybrowserandsubs" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Browser for YIFY</name>
    <description>Browser for YIFY</description>
    <author email="ravi7mech@gmail.com">Ravikumar R</author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="https://itunes.apple.com" />
    <access origin="https://play.google.com" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="xxxhdpi" src="resources/android/drawable/icon.png" />
        <icon density="ldpi" src="resources/android/drawable-ldpi/icon.png" />
        <icon density="mdpi" src="resources/android/drawable-mdpi/icon.png" />
        <icon density="hdpi" src="resources/android/drawable-hdpi/icon.png" />
        <icon density="xhdpi" src="resources/android/drawable-xhdpi/icon.png" />
        <icon density="xxhdpi" src="resources/android/drawable-xxhdpi/icon.png" />
        <icon density="xxxhdpi" src="resources/android/drawable-xxxhdpi/icon.png" />
        <splash density="port-hdpi" src="resources/android/screen.png" />
        <splash density="port-ldpi" src="resources/android/screen.png" />
        <splash density="port-mdpi" src="resources/android/screen.png" />
        <splash density="port-xhdpi" src="resources/android/screen.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashScreenDelay" value="7000" />
    <preference name="SplashScreen" value="screen" />
    <preference name="Orientation" value="portrait" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <allow-navigation href="http://192.168.43.253:8100" />
    <plugin name="cordova-plugin-device" spec="^1.1.6" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="^2.2.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="ionic-plugin-keyboard" spec="^2.2.1" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.1" />
    <plugin name="cordova-plugin-x-socialsharing" spec="^5.2.0" />
    <plugin name="cordova-plugin-speechrecognition" spec="^1.1.2" />
    <plugin name="cordova-plugin-appavailability" spec="^0.4.2" />
    <plugin name="cordova-plugin-email-composer" spec="^0.8.10" />
    <plugin name="cordova-plugin-network-information" spec="^1.3.3" />
    <plugin name="cordova-plugin-dialogs" spec="^1.3.3" />
    <plugin name="cordova-plugin-apprate" spec="^1.3.0" />
    <plugin name="cordova-clipboard" spec="^1.1.0" />
    <plugin name="phonegap-plugin-push" spec="^2.1.1">
        <variable name="SENDER_ID" value="275251668763" />
    </plugin>
    <plugin name="cordova-plugin-firebase-analytics" spec="^0.12.1">
        <variable name="FIREBASE_VERSION" value="11.8.+" />
    </plugin>
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.0" />
    <plugin name="cordova-admob-sdk" spec="~0.13.1">
        <variable name="PLAY_SERVICES_VERSION" value="11.8.0" />
    </plugin>
    <plugin name="cordova.plugins.diagnostic" spec="^3.9.0" />
    <plugin name="cordova-plugin-x-toast" spec="^2.6.2" />
    <plugin name="cordova-plugin-market" spec="^1.2.0" />
    <engine name="android" spec="6.3.0" />
</widget>
